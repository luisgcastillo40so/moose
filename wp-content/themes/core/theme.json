{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "background": false,
      "link": false,
      "palette": [
        {
          "color": "#ffffff",
          "name": "Base White",
          "slug": "base-white"
        },
        {
          "color": "#000000",
          "name": "Base Black",
          "slug": "base-black"
        },
        {
          "color": "#3050E5",
          "name": "Brand Primary",
          "slug": "brand-primary"
        },
        {
          "color": "#2238a0",
          "name": "Brand Secondary",
          "slug": "brand-secondary"
        },
        {
          "color": "#f5f5f5",
          "name": "Neutral Gray 10",
          "slug": "neutral-10"
        },
        {
          "color": "#e8e8e8",
          "name": "Neutral Gray 20",
          "slug": "neutral-20"
        },
        {
          "color": "#d1d1d1",
          "name": "Neutral Gray 30",
          "slug": "neutral-30"
        },
        {
          "color": "#a3a3a3",
          "name": "Neutral Gray 40",
          "slug": "neutral-40"
        },
        {
          "color": "#8c8c8c",
          "name": "Neutral Gray 50",
          "slug": "neutral-50"
        },
        {
          "color": "#747474",
          "name": "Neutral Gray 60",
          "slug": "neutral-60"
        },
        {
          "color": "#5d5d5d",
          "name": "Neutral Gray 70",
          "slug": "neutral-70"
        },
        {
          "color": "#464646",
          "name": "Neutral Gray 80",
          "slug": "neutral-80"
        },
        {
          "color": "#2f2f2f",
          "name": "Neutral Gray 90",
          "slug": "neutral-90"
        },
        {
          "color": "#f1c400",
          "name": "Warning",
          "slug": "warning"
        },
        {
          "color": "#df3a3a",
          "name": "Error",
          "slug": "error"
        },
        {
          "color": "#84bd00",
          "name": "Success",
          "slug": "success"
        }
      ]
    },
    "custom": {
      "fontWeight": {
        "regular": 400,
        "bold": 700
      }
    },
    "layout": {
      "contentSize": "80ch",
      "wideSize": "1200px"
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fluid": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "fontFamilies": [
        {
          "fontFamily": "\"Inter\"",
          "name": "Inter",
          "slug": "inter",
          "fontFace": [
            {
              "fontFamily": "\"Inter-Regular\"",
              "fontDisplay": "swap",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/Inter-Regular.woff2",
                "file:./assets/fonts/Inter-Regular.woff"
              ]
            },
            {
              "fontFamily": "\"Inter-Bold\"",
              "fontDisplay": "swap",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/Inter-Bold.woff2",
                "file:./assets/fonts/Inter-Bold.woff"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "slug": "type-90",
          "size": "clamp(3rem, 2.272rem + 3.107vw, 6rem)",
          "name": "Type Size 90"
        },
        {
          "slug": "type-80",
          "size": "clamp(2.5rem, 1.954rem + 2.33vw, 4.75rem)",
          "name": "Type Size 80"
        },
        {
          "slug": "type-70",
          "size": "clamp(2rem, 1.515rem + 2.071vw, 4rem)",
          "name": "Type Size 70"
        },
        {
          "slug": "60",
          "size": "clamp(1.5rem, 1.136rem + 1.553vw, 3rem)",
          "name": "Type Size 60"
        },
        {
          "slug": "50",
          "size": "clamp(1.375rem, 1.102rem + 1.165vw, 2.5rem)",
          "name": "Type Size 50"
        },
        {
          "slug": "40",
          "size": "clamp(1.25rem, 1.068rem + 0.777vw, 2rem)",
          "name": "Type Size 40"
        },
        {
          "slug": "30",
          "size": "clamp(1.125rem, 1.034rem + 0.388vw, 1.5rem)",
          "name": "Type Size 30"
        },
        {
          "slug": "20",
          "size": "clamp(1rem, 0.939rem + 0.259vw, 1.25rem)",
          "name": "Type Size 20"
        },
        {
          "slug": "10",
          "size": "clamp(0.875rem, 0.845rem + 0.129vw, 1rem)",
          "name": "Type Size 10"
        }
      ]
    },
    "shadow": {
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
        },
        {
          "name": "Deep",
          "slug": "deep",
          "shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)"
        }
      ]
    },
    "blocks": {
      "core/heading": {
        "color": {
          "palette": [
            {
              "color": "#000000",
              "name": "Foreground - Contrast Light",
              "slug": "foreground-contrast-light"
            },
            {
              "color": "#ffffff",
              "name": "Foreground - Contrast Dark",
              "slug": "foreground-contrast-dark"
            }
          ]
        }
      },
      "core/paragraph": {
        "color": {
          "palette": [
            {
              "color": "#000000",
              "name": "Foreground - Contrast Light",
              "slug": "foreground-contrast-light"
            },
            {
              "color": "#ffffff",
              "name": "Foreground - Contrast Dark",
              "slug": "foreground-contrast-dark"
            }
          ]
        }
      }
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--base-black)",
      "background": "var(--wp--preset--color--base-white)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--20)",
      "fontStyle": "normal",
      "fontWeight": "var(--wp--custom--font-weight--regular)",
      "lineHeight": "1.5"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--20)",
          "fontWeight": "var(--wp--custom--font-weight--regular)",
          "lineHeight": "1.5"
        }
      }
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--inter)",
          "fontWeight": "var(--wp--custom--font-weight--bold)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--80)",
          "lineHeight": "1.1"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--70)",
          "lineHeight": "1.12"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--60)",
          "lineHeight": "1.15"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--50)",
          "lineHeight": "1.2"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--40)",
          "lineHeight": "1.25"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--30)",
          "lineHeight": "1.4"
        }
      },
      "caption": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--10)",
          "fontWeight": "var(--wp--custom--font-weight--regular)",
          "lineHeight": "1.65"
        }
      },
      "cite": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--20)",
          "fontWeight": "var(--wp--custom--font-weight--regular)",
          "lineHeight": "1.5"
        }
      }
    }
  }
}
