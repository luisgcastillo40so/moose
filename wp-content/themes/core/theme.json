{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "color": "#3050E5",
          "name": "Brand Primary",
          "slug": "brand-primary"
        },
        {
          "color": "#2238a0",
          "name": "Brand Secondary",
          "slug": "brand-secondary"
        },
        {
          "color": "#ffffff",
          "name": "Background - Light",
          "slug": "background-light"
        },
        {
          "color": "#f5f5f5",
          "name": "Background - Subtle Light",
          "slug": "background-subtle-light"
        },
        {
          "color": "#000000",
          "name": "Background - Dark",
          "slug": "background-dark"
        },
        {
          "color": "#000000",
          "name": "Foreground - Contrast Light",
          "slug": "foreground-contrast-light"
        },
        {
          "color": "#ffffff",
          "name": "Foreground - Contrast Dark",
          "slug": "foreground-contrast-dark"
        }
      ]
    },
    "layout": {
      "contentSize": "80ch",
      "wideSize": "1200px"
    },
    "shadow": {
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
        },
        {
          "name": "Deep",
          "slug": "deep",
          "shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--foreground-contrast-light)",
      "background": "var(--wp--preset--color--background-light)"
    }
  }
}
